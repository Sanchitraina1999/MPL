;Write X86/64 ALP to perform non-overlapped and overlapped block transfer (with and without 
;string specific instructions). Block containing data can be defined in the data segment.

;# NON-OVERLAPPING BLOCK TRANSFER

%macro print 2
	mov rax,01
	mov rdi,01
	mov rsi,%1
	mov rdx,%2
	syscall
%endmacro

section .data
	array db 10h,20h,30h,40h,50h

	msg1 db "Before overlapping :",0xA
	len1 equ $-msg1
	
	msg2 db "After overlapping :",0xA
	len2 equ $-msg2

	msg3 db " ",0xA
	len3 equ $-msg3
	
	msg4 db ":     "
	len4 equ $-msg4


section .text
	global _start
_start:
	print msg1,len1

	
	

	print msg2,len2

	mov rax,60
	mov rdi,00
	syscall
	
	
